<div class = "generalcontent">

   <h2>Edit Tag</h2>

   <div class = "generalbox">

      <% form_for(@tag) do |f| %>
        <%= f.error_messages %>

        <p>
          <%= f.label :name %><br />
          <%= f.text_field :name %>
        </p>
        <p>
          <%= f.submit "Update" %>
        </p>
      <% end %>
   </div>  <!-- End class="generalbox" -->
</div>  <!-- End class="generalcontent" -->
