<div class = "generalcontent">
    
    <div id = "new_tag">
      <br>
      <% form_for :tag, @tag, {:url => { :action => "create" }, :html  => {:class  => "ajax_form", "data-datatype" => "script"}}  do |f| -%>
        <%= f.error_messages %>
          <%= f.label :name %><br />
          <%= f.text_field :name %>
          <%= f.submit "Create" %>
      <% end %>
      <br>
     </div>
</div>   <!-- End class="generalcontent" -->
