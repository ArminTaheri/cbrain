<% html_tool_tip status_html_symbol(syncstat.status) do %>
  <%= syncstat.remote_resource.class.to_s + " '" + syncstat.remote_resource.name + "' : " + status_html_symbol(syncstat.status) + " (" + syncstat.status + ")" %>
  <% if syncstat.status == 'InSync' -%>
    <br>
    Last Synchronized Date: <%= pretty_past_date syncstat.synced_at %><br>
    Last Accessed Date:     <%= pretty_past_date syncstat.accessed_at %>
  <% end -%>
<% end %>
