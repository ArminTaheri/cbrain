<div id="new_model_panel">
  
  <%= ajax_form_for @bourreau, :as => :bourreau, :url => { :action => "create" }, :datatype => "script" do |f| -%>

     <%= f.error_messages %>

     <%= render :partial => 'form_fields', :locals => { :f => f }  %>

     <p><%= submit_tag 'Create' %></p>

  <% end -%>

  <%= render :partial => "notes" %>
</div>


