-%>
<html>

  <head>
    <title>CBRAIN Project <%= yield :title %></title>
    <link rel="shortcut icon" type="image/giv" href="/images/favicon.gif" />
    <%= stylesheet_link_tag  "cbrain","niftyCorners", "jquery-ui", "macacc", :media => "all" %>
    <%= javascript_include_tag :defaults %>
    <%= javascript_include_tag "tableH" %>
    <%= javascript_include_tag "niftycube" %>
    <script type="text/javascript" src="/javascripts/o3djs/base.js"></script>
    <%= javascript_include_tag "jquery", "jquery-ui", "jquery.form", "processing" %>
    <%= javascript_include_tag "brainbrowser", "mniobj", "dataset", "macacc_collection", "civet", "thickness" %> 
    <%= javascript_include_tag "cbrain" %>
    <script type="text/javascript">
      jQuery.noConflict();
    </script>
  </head>

  <body id="cbrain">     
    <%= render :partial => 'layouts/section_account' %>
    <%= render :partial => 'layouts/section_menu'    %>
    <%= render :partial => 'layouts/section_main'    %>
    <%= render :partial => 'layouts/section_footer'  %>
  </body>

</html>
